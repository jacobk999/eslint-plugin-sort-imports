{
  "name": "eslint-plugin-sort-imports",
  "version": "1.0.0",
  "description": "A sort-imports rule that properly distinguishes between ES6 import types.",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "sort-imports",
    "import-order"
  ],
  "author": "marudor",
  "contributors": [
    {
      "name": "Jacob Koshy",
      "email": "jababakoshy@gmail.com",
      "url": "https://github.com/jacobk999"
    }
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jacobk999/eslint-plugin-sort-imports"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    "default": "./dist/index.js",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.1.0",
    "@typescript-eslint/rule-tester": "^8.1.0",
    "@typescript-eslint/utils": "^8.1.0",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "license": "MIT"
}
